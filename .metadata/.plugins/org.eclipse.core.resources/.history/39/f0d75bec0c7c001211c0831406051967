
public class Coffee {
	private int id;
	private String name;
	private int price_small;
	private int price_medium;
	private int price_big;
	public int getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public int getPrice_small() {
		return price_small;
	}
	public int getPrice_medium() {
		return price_medium;
	}
	public int getPrice_big() {
		return price_big;
	}
	public void setId(int id) {
		this.id = id;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setPrice_small(int price_small) {
		this.price_small = price_small;
	}
	public void setPrice_medium(int price_medium) {
		this.price_medium = price_medium;
	}
	public void setPrice_big(int price_big) {
		this.price_big = price_big;
	}
	public Coffee(int id, String name, int price_small, int price_medium,
			int price_big) {
		super();
		this.id = id;
		this.name = name;
		this.price_small = price_small;
		this.price_medium = price_medium;
		this.price_big = price_big;
	}

	
}
